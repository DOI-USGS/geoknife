<wps:Execute xmlns:wps="{{wps}}" xmlns:ows="{{ows}}" xmlns:ogc="{{ogc}}" xmlns:xlink="{{xlink}}" xmlns:xsi="{{xsi}}" service="{{service}}" version="{{version}}" xsi:schemaLocation="{{schema_location}}">
  <ows:Identifier>{{identifier}}</ows:Identifier>
  <wps:DataInputs>
{{#inputs}}
    <wps:Input>
      <ows:Identifier>{{input_identifier}}</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{input_literal_data_element}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
{{/inputs}}
  </wps:DataInputs>
  <wps:ResponseForm>{{#responsedocument}}
    <wps:ResponseDocument>
      <wps:Output>
        <ows:Identifier>{{result_name}}</ows:Identifier>
      </wps:Output>
    </wps:ResponseDocument>{{/responsedocument}}{{#rawoutput}}
    <wps:RawDataOutput>
      <ows:Identifier>{{result_name}}</ows:Identifier>
    </wps:RawDataOutput>{{/rawoutput}}
  </wps:ResponseForm>
</wps:Execute> 
