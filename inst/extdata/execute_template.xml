<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">
  <ows:Identifier>{{identifier}}</ows:Identifier>
  <wps:DataInputs>
    {{#inputs}}
    <wps:Input>
      <ows:Identifier>{{input_identifier}}</ows:Identifier>
      {{#data_elements}}
      <wps:Data>
        <wps:LiteralData>{{input_literal_data_element}}</wps:LiteralData>
      </wps:Data>
      {{/data_elements}}
    </wps:Input>
    {{/inputs}}
    <wps:Input>
      <ows:Identifier>FEATURE_COLLECTION</ows:Identifier>
      <wps:Reference xlink:href="{{wps_reference_href}}">
        <wps:Body>
          <wfs:GetFeature xmlns:wfs="{{wfs_namespace}}" xmlns:gml="{{gml_namespace}}" service="{{wfs_service}}" version="{{wfs_version}}" outputFormat="{{wfs_outputformat}}">
            <wfs:Query typeName="{{wfs_typename}}">
              <wfs:PropertyName>{{wfs_geom_property}}</wfs:PropertyName>
              <wfs:PropertyName>{{wfs_attribute_property}}</wfs:PropertyName>
              {{#filter_gmlid}}
              <ogc:Filter>
                {{#gmlids}}
                <ogc:GmlObjectId gml:id="{{gmlid}}"/>
                {{/gmlids}}
              </ogc:Filter>
              {{/filter_gmlid}}
            </wfs:Query>
          </wfs:GetFeature>
        </wps:Body>
      </wps:Reference>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:ResponseDocument storeExecuteResponse="{{storeExecuteResponse}}" status="{{status}}">
      <wps:Output asReference="{{asReference}}">
        <ows:Identifier>{{output_identifier}}</ows:Identifier>
      </wps:Output>
    </wps:ResponseDocument>
  </wps:ResponseForm>
</wps:Execute>
