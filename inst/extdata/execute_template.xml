<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:ogc="http://www.opengis.net/ogc" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" service="WPS" version="1.0.0" xsi:schemaLocation="http://www.opengis.net/wps/1.0.0 http://schemas.opengis.net/wps/1.0.0/wpsExecute_request.xsd">
  <ows:Identifier>{{identifier}}</ows:Identifier>
  <wps:DataInputs>
    {{#inputs}}
    <wps:Input>
      <ows:Identifier>{{input_identifier}}</ows:Identifier>
      {{#data_elements}}
      <wps:Data>
        <wps:LiteralData>{{input_literal_data_element}}</wps:LiteralData>
      </wps:Data>
      {{/data_elements}}
    </wps:Input>
    {{/inputs}}
    <wps:Input>
      <ows:Identifier>FEATURE_COLLECTION</ows:Identifier>
      <wps:Reference xlink:href="https://cida.usgs.gov/gdp/geoserver/wfs">
        <wps:Body>
          <wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml" service="WFS" version="1.1.0" outputFormat="text/xml; subtype=gml/3.1.1">
            <wfs:Query typeName="sample:CONUS_states">
              <wfs:PropertyName>the_geom</wfs:PropertyName>
              <wfs:PropertyName>STATE</wfs:PropertyName>
              <ogc:Filter>
                <ogc:GmlObjectId gml:id="CONUS_states.48"/>
              </ogc:Filter>
            </wfs:Query>
          </wfs:GetFeature>
        </wps:Body>
      </wps:Reference>
    </wps:Input>
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:ResponseDocument storeExecuteResponse="true" status="true">
      <wps:Output asReference="true">
        <ows:Identifier>OUTPUT</ows:Identifier>
      </wps:Output>
    </wps:ResponseDocument>
  </wps:ResponseForm>
</wps:Execute>
